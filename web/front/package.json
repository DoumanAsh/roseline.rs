{
    "name": "roseline-front",
    "version": "1.0.0",
    "private": true,
    "description": "Front-end for Roseline",
    "main": "index.js",
    "scripts": {
        "test": "ava --verbose",
        "lint:style": "stylelint src/styles",
        "lint:js": "eslint . --ext .js --ignore-pattern public",
        "lint": "npm run lint:style && npm run lint:js",
        "start": "webpack-dev-server",
        "build": "webpack",
        "build:p": "webpack -p",
        "build:profile": "webpack --profile --json > stats.json",
        "build:analyze": "webpack-bundle-analyzer stats.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DoumanAsh/roseline.rs.git"
    },
    "keywords": [],
    "author": "Douman <douman@gmx.se>",
    "license": "MIT",
    "dependencies": {
        "blaze": "3"
    },
    "devDependencies": {
        "autoprefixer": "^7.2.6",
        "babel-core": "6",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "1",
        "css-loader": "^0.28.11",
        "eslint": "^4.19.1",
        "eslint-loader": "1",
        "eslint-plugin-compat": "2",
        "extract-text-webpack-plugin": "3",
        "file-loader": "^1.1.11",
        "html-loader": "0",
        "html-webpack-plugin": "2",
        "json5-loader": "1",
        "postcss-import": "^11.1.0",
        "postcss-loader": "^2.1.4",
        "postcss-nesting": "4",
        "pug": "^2.0.3",
        "pug-loader": "^2.4.0",
        "purecss": "1",
        "raw-loader": "0",
        "script-ext-html-webpack-plugin": "1",
        "stylelint": "8",
        "stylelint-config-standard": "^18.2.0",
        "sugarss": "1",
        "webpack": "^3.11.0",
        "webpack-dev-server": "^2.11.2"
    },
    "ava": {
        "files": [
            "test/**/*.js"
        ],
        "source": [
            "src/**/*.js"
        ],
        "failFast": true
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "modules": false
                }
            ]
        ]
    },
    "browserslist": [
        "Last 2 versions"
    ]
}
